{
  "name": "website",
  "private": true,
  "description": "project website",
  "keywords": [
    "javascript",
    "react",
    "uikit",
    "react-component",
    "react-uikit",
    "uikit-react"
  ],
  "repository": "https://github.com/otissv/react-uikit-components/tree/master/packages/react-uikit-components",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "./docs/scripts/bundle.js",
  "scripts": {
    "build": "NODE_ENV='website' webpack --config ./webpack.config-build.js --progress",
    "lint": "eslint --quiet ./src/**  && date || true && date",
    "lint:watch": "watch 'npm run lint' ./",
    "uikit": "postcssbuild --config postcssbuild.json --d src/styles/uikit -o docs/styles/uikit.min.css",
    "css": "postcssbuild --config postcssbuild.json --s src/styles/main.css -o docs/styles/main.min.css",
    "css:build": "npm run uikit && npm run css",
    "prepublish": "npm run build && npm run css:build",
    "start": "node ./server.js",
    "webpack": "webpack",
    "watch": "webpack --watch"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-cli": "~6.11.4",
    "babel-core": "~6.13.2",
    "babel-eslint": "~6.1.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-plugin-transform-runtime": "~6.12.0",
    "babel-preset-es2015": "~6.13.2",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-0": "~6.16.0",
    "babel-runtime": "~6.11.6",
    "babelify": "~7.3.0",
    "browser-sync": "~2.17.3",
    "concurrently": "^3.4.0",
    "eslint": "~3.3.1",
    "eslint-config-standard": "~5.3.5",
    "eslint-loader": "~1.5.0",
    "eslint-plugin-promise": "~2.0.1",
    "eslint-plugin-react": "~6.1.1",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~2.0.0",
    "history": "~3.0.0",
    "json-loader": "~0.5.4",
    "postcss-build": "~1.0.6",
    "react-addons-test-utils": "~15.3.0",
    "react-dom": "~15.3.2",
    "react-transform-catch-errors": "~1.0.2",
    "react-transform-hmr": "~1.0.4",
    "redbox-react": "~1.3.2",
    "tap-difflet": "~0.6.0",
    "tap-min": "~1.1.0",
