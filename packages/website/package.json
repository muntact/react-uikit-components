    "uglify-loader": "~1.3.0",
    "watch": "~0.19.1",
    "webpack": "~1.13.0",
    "webpack-dev-middleware": "~1.8.4",
    "webpack-hot-middleware": "~2.13.0"
  },
  "dependencies": {
    "cuid": "~1.3.8",
